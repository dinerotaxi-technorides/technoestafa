  body
    div(ng-include="'/common/views/partials/version.html'")
    // Preloader
    #preloader
      #status
        i.fa.fa-spinner.fa-spin.fa-3x
    section
      .leftpanel
        .logopanel
          h1 
            p Techno
              span Rides
        // logopanel
        .leftpanelinner
          // This is only visible to small devices
          .visible-xs.hidden-sm.hidden-md.hidden-lg
            .media.userlogged
              img.media-object(alt="", src="/common/assets/user/profile.png")
              .media-body
                h4 {{user.firstName}} {{user.lastName}}
            ul.nav.nav-pills.nav-stacked.nav-bracket.mb30
             
              li
                a(href="",ng-click="user.logout()")
                  i.fa.fa-power-off
                  | Log out
          h5
          ul.nav.nav-pills.nav-stacked.nav-bracket
            li(ng-class="{active: route.current.activetab == 'cities'}")
              a(href="#cities")
                i.fa.fa-globe
                span Cities
            
            li(ng-class="{active: route.current.activetab == 'config'}")
              a(href="#configuration")
                i.fa.fa-cog
                span Configuration
            
            li(ng-class="{active: route.current.activetab == 'emarketing'}")
              a(href="#e-marketing")
                i.fa.fa-envelope
                span Email Marqueting
            
            li(ng-class="{active: route.current.activetab == 'employees'}")
              a(href="#employees")
                i.fa.fa-users
                span Clients Employees

            li(ng-class="{active: route.current.activetab == 'clients'}")
              a(href="#clients")
                i.fa.fa-user
                span Clients

            li(ng-class="{active: route.current.activetab == 'users'}")
              a(href="#users")
                i.fa.fa-user-secret
                span Users
            li(ng-class="{active: route.current.activetab == 'message'}")
              a(href="#message")
                i.fa.fa-bullhorn
                span Messages
      
      .mainpanel
        .headerbar
          a.menutoggle
            i.fa.fa-bars
          .header-right
            ul.headermenu       
              li
                .btn-group
                  button.btn.btn-default.dropdown-toggle(type="button", data-toggle="dropdown")
                    img(src="/common/assets/user/profile.png", alt="")
                    | {{user.firstName}} {{user.lastName}}
                    span.caret
                  ul.dropdown-menu.dropdown-menu-usermenu.pull-right
                    li
                      a(href="#", ng-click="user.logout()")
                        i.fa.fa-power-off
                        | Log Out
        div(ng-view="")
        div(ng-include="'/common/views/partials/modals/feedback.html'")
        
  div(ng-include="'/common/views/partials/modals/messages.html'")
  
