section.header(style="background-image: url('assets/driver/bg.jpg');height: 700px")#home
  .inner-video
    .container
      .col-xs-12.inner-text.text-center
        h1.uppercase
          span(typed-translate="") passenger.separatedTitle1
          span &nbsp;
          span.special(typed-translate="") passenger.separatedTitle2
          span ,
          span &nbsp;
          span(typed-translate="") passenger.separatedTitle3
        p(typed-translate="") passenger.subTitle
        a.orange-btn.uppercase(style="margin-right:5px" href="#signup" typed-translate="" ng-click="user.userType = 'passenger'") passenger.header.button1 
        a.inverse-white.uppercase(typed-translate="" href="#advantages" du-smooth-scroll duration="2000") passenger.header.button2
  video(autoplay="true" loop="true" style="margin:0 auto;bottom: 431px;" )
    source(src="assets/videos/passenger_darker.mp4" type="video/mp4")
.clearfix
article.apps
  .container
    span.pull-left.hidden-xs(style="position:relative;top30px" typed-translate="") home.downloadApps
    a.btn-app(href="{{company.info.android}}" target="_blank")
      img(src="assets/playstore.png")
    a.btn-app(href="{{company.info.ios}}" target="_blank")
      img(src="assets/appstore.png")

