section.howWorkPassenger#howWorks
  .container
    h2.uppercase.text-center(typed-translate="") index.sections.howItWorks
    .text-center
    .clearfix
    .row.feature
      .col-xs-12.animate-switch
        .col-xs-12.animated
          .feature-body
            .col-xs-6.col-md-3.how-works-item
              .media
                .text-center
                  img.media-object.img-circle(src="assets/enterprise/ic_company_signup.png" style="margin: 0 auto;")
                .media-body(style="padding-top:30px")
                  h4.media-heading.uppercase.text-center(typed-translate="") passenger.howitWorks.step1.title
                  p.text-center
                    a.special(href="#signup" ng-click="user.userType = 'enterprise'")
                      span(typed-translate="") index.sections.signup
                      span &nbsp;
                    span(typed-translate="passenger.howitWorks.step1.text" translate-values='{company: company.info.name}')  
            .col-xs-6.col-md-3.how-works-item  
              .media
                .text-center
                  img.media-object.img-circle(src="assets/enterprise/ic_company_training.png" style="margin: 0 auto;")
                .media-body(style="padding-top:30px")
                  h4.media-heading.uppercase.text-center(typed-translate="") passenger.howitWorks.step2.title
                  p.text-center(typed-translate="")  passenger.howitWorks.step2.text
            .col-xs-6.col-md-3.how-works-item
              .media
                .text-center
                  img.media-object.img-circle(src="assets/enterprise/ic_company_platformsetup.png" style="margin: 0 auto;") 
                .media-body(style="padding-top:30px")
                  h4.media-heading.uppercase.text-center(typed-translate="") passenger.howitWorks.step3.title
                  p.text-center(typed-translate="passenger.howitWorks.step3.text" translate-values='{company: company.info.name}') 
            .col-xs-6.col-md-3.how-works-item
              .media
                .text-center
                  img.media-object.img-circle(src="assets/enterprise/ic_company_addemployees.png" style="margin: 0 auto;") 
                .media-body(style="padding-top:30px")
                  h4.media-heading.uppercase.text-center(typed-translate="") passenger.howitWorks.step4.title
                  p.text-center(typed-translate="") passenger.howitWorks.step4.text
    .row
      .col-xs-12
      img.img-responsive(ng-if="company.info.rtaxi != '98280' && company.info.businessModel == 'GENERIC'" src="assets/passenger/howitworks_feature.png")
      img.img-responsive(ng-if="company.info.rtaxi != '98280' && company.info.businessModel != 'GENERIC'" src="assets/passenger/howitworks_feature_moto.png")
      img.img-responsive(ng-if="company.info.rtaxi == '98280'" src="assets/passenger/howitworks_feature_et.png")
      
