doctype html
html(ng-app="technoridesApp", lang="{{lang}}" ng-controller="technoridesCtrl")
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1")
    link(rel="icon" href="/common/assets/logo/favicon.ico" type="image/x-icon")
    meta(name="description" content="")
    title {{'siteTitle' | translate}}

    // Bootstrap
    link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css")

    // Angular
    script(src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.21/angular.js")
    script(src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.21/angular-animate.min.js")
    script(src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.21/angular-route.js")
    script(src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.21/angular-cookies.js")
    script(src="//cdn.jsdelivr.net/angular.translate/2.1.0/angular-translate.min.js")
    script(src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.2/angular-sanitize.js")

    // Map Dependencies
    script(src="https://maps.googleapis.com/maps/api/js?v=3&libraries=places&sensor=false")
    script(src='https://api.mapbox.com/mapbox.js/v2.4.0/mapbox.js')
    link(href='https://api.mapbox.com/mapbox.js/v2.4.0/mapbox.css' rel='stylesheet')
    script(src="https://api.mapbox.com/mapbox.js/plugins/leaflet-label/v0.2.1/leaflet.label.js")
    link(href='https://api.mapbox.com/mapbox.js/plugins/leaflet-label/v0.2.1/leaflet.label.css' rel='stylesheet')
    
    script(src='https://api.mapbox.com/mapbox.js/plugins/leaflet-markercluster/v0.4.0/leaflet.markercluster.js')
    link(href='https://api.mapbox.com/mapbox.js/plugins/leaflet-markercluster/v0.4.0/MarkerCluster.css' rel='stylesheet')
    link(href='https://api.mapbox.com/mapbox.js/plugins/leaflet-markercluster/v0.4.0/MarkerCluster.Default.css' rel='stylesheet')

    // Jquery
    script(src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js")
    script(src="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js")

    // Tooltip
    script(src="/common/libs/tooltip-angular.js")

    // Config
    script(src="/technorides/scripts/technorides.js")
    script(src="/technorides/scripts/config/routes.js")

    // Api
    script(src="/common/scripts/api/api.js")
    script(src="/technorides/scripts/services/api/apiHandler.js")
    script(src="/technorides/scripts/services/api/apiAdapter.js")

    // Controller
    script(src="/technorides/scripts/controllers/technoridesCtrl.js")

    // Settings
    script(src="/common/scripts/settings.js")
    script(src="/technorides/scripts/config/locale.js")

    // Main styles
    link(rel="stylesheet" href="/technorides/styles/styles.css")

    // Scroll
    script(src="/common/libs/angular-scroll.min.js")

    // Services
    script(src="/common/scripts/messages.js")

    // Fake services to avoid injection error on $map
    script(src="/technorides/scripts/services/fake-company.js")
    script(src="/technorides/scripts/services/fake-user.js")
    
    
    //map services
    script(src="/common/scripts/mapbox.js")
    script(src="/common/scripts/map.js")
    script(src="/common/scripts/google.js")
    
    // Angular-select
    script(src="/common/libs/angular-strap.min.js")


    // get country
    script(src="http://www.geoplugin.net/javascript.gp")
    // HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js")
      script(src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js")
    link(href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css')
    link(href='https://fonts.googleapis.com/css?family=Exo:400,400italic' rel='stylesheet' type='text/css')
    link(href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet")

    // Analytics
    script(aync src="https://www.google-analytics.com/analytics.js")
    script(src="/common/libs/angulartics.min.js")
    script(src="/common/libs/angulartics-ga.min.js")
