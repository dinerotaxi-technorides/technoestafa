      img(height="1" width="1" style="display:none"
      src="https://www.facebook.com/tr?id=1700222866892100&ev=PageView&noscript=1")

  body
      div(ng-include="'/common/views/partials/version.html'")
      nav.navbar.navbar-fixed-top.shadow-bottom(role="navigation")
        .nav-small
          .container
            .btn-group
              button.btn.btn-default.dropdown-toggle.btn-sm(type="button" data-toggle="dropdown" aria-expanded="false")
                img(ng-src="/technorides/assets/lang/{{lang}}.png")
                span(ng-if="lang == 'es'") Español
                span(ng-if="lang == 'en'") English
                span(ng-if="lang == 'fr'") Francais
                span.caret
              ul.dropdown-menu(role="menu")
                li
                  a(ng-click="changeLanguage('en')")
                    img(ng-src="/technorides/assets/lang/en.png")
                    | English
                li
                  a(ng-click="changeLanguage('es')")
                    img(ng-src="/technorides/assets/lang/es.png")
                    | Español
                li
                  a(ng-click="changeLanguage('fr')")
                    img(ng-src="/technorides/assets/lang/fr.png")
                    | Francais
            ul.user-actions.pull-right
              li
                a(href="/login" rel="nofollow") {{'SignIn' | translate}}
              li
                a |
              li
                a(href="/signup" rel="nofollow") {{'SignUp' | translate}}
        .container
          .navbar-header
            button.navbar-toggle.collapsed(type="button", data-toggle="collapse", data-target="#collapse-button")
              span.sr-only Toggle navigation
              i.fa.fa-bars.fa-2x

            a.navbar-brand(href="#")
              img.pull-left(src="/common/assets/logo/logo-sm.png")
              p.pull-left Techno
                span Rides
          .collapse.navbar-collapse#collapse-button
            ul.nav.navbar-nav.navbar-right#main-nav
              li(du-scrollspy="top")
                a(href="#top" du-smooth-scroll) {{'Home' | translate}}
              li(du-scrollspy="features")
                a(href="#features" du-smooth-scroll) {{'Features' | translate}}
              li(du-scrollspy="costumers")
                a(href="#costumers" du-smooth-scroll) {{'Costumers' | translate}}
              li(du-scrollspy="pricing")
                a(href="#pricing" du-smooth-scroll) {{'Pricing' | translate}}
              li(du-scrollspy="contact")
                a(href="#contact" du-smooth-scroll) {{'Contact' | translate}}
              li.active.hidden-sm
                a(ng-click="openFreeTrial('none')" style="cursor:pointer" analytics-category="web" analytics-on="click"  analytics-label="click on the navbar free trial" analytics-event="free trial menu") {{'FreeTrial' | translate}}
      div#container(ng-include="'/technorides/views/home.html'")
      .cars-footer
      footer
        .container
          .col-xs-12.col-lg-4
            h4.uppercase {{'navigate' | translate}}
            ul
              li(du-scrollspy="pricing").hidden
                a(href="#pricing" du-smooth-scroll) {{'Pricing' | translate}}
              li {{'SignUp' | translate}}
              li {{'SupportFAQ' | translate}}
              li {{'Team' | translate}}
              li {{'PrivacyPolicy' | translate}}
              li {{'TermsConditions' | translate}}
              li {{'Blog' | translate}}
          .col-xs-12.col-lg-4
            h4.uppercase {{'contactFooter' | translate}}
            a(href="https://www.facebook.com/technorides" target="_blank")
              img.img-circle(src="/technorides/assets/facebook.png")
            a(href="https://twitter.com/techno_rides" target="_blank")
              img.img-circle(src="/technorides/assets/twitter.png")
            a(href="https://www.linkedin.com/company/dinerotaxi?trk=vsrp_companies_res_name&trkInfo=VSRPsearchId%3A2535370261420746991667%2CVSRPtargetId%3A2648048%2CVSRPcmpt%3Aprimary" target="_blank")
              img.img-circle(src="/technorides/assets/linkedin.png")
            a(href="https://angel.co/technorides" target="_blank")
              img.img-circle(src="/technorides/assets/angellist.png")
            br
            br
            form
              label {{'SuscribeNews' | translate}}
              .input-group
                input.form-control(type="text")
                span.input-group-btn
                  button.btn.btn-default(type="button")
                    i.fa.fa-caret-right
          .col-xs-12.col-lg-4.about-us
            h4.uppercase {{'aboutUs' | translate}}
            p {{'aboutUsTextFooter' | translate}}
            br
            br
            small.copy &copy; {{'copyRight' | translate}}
    div(ng-include="'/technorides/views/free-trial.html'")
  div(ng-include="'/technorides/views/products.html'")
  div(ng-include="'/common/views/partials/modals/messages.html'")
